{
  "name": "formik-antd",
  "version": "2.0.0-beta.0",
  "author": "Jannik Buschke",
  "description": "Dead simple declarative bindings for Formik and Ant Design.",
  "displayName": "formik-antd",
  "keywords": [
    "react",
    "formik",
    "antd",
    "ant",
    "design",
    "form"
  ],
  "sideEffects": [
    "es/**/style/*",
    "lib/**/style/*"
  ],
  "license": "MIT",
  "private": false,
  "files": [
    "lib",
    "src",
    "es"
  ],
  "module": "./es/index.js",
  "main": "./lib/index.js",
  "types": "./lib/types.d.ts",
  "scripts": {
    "build": "tsc && tsc -p tsconfig.es.json",
    "tsc": "tsc && tsc -p tsconfig.es.json",
    "watch": "tsc --watch",
    "cleanbuild": "rm -r node_modules && npm install && tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --write src/**/*.* package.json"
  },
  "dependencies": {},
  "peerDependencies": {
    "antd": ">= 3.12.1 < 4",
    "formik": ">= 1.4.1 < 3",
    "react": ">= 16.8.0 < 17",
    "react-dom": ">= 16.8.0 < 17"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "4.2.0",
    "@testing-library/react": "9.3.0",
    "@types/jest": "24.0.19",
    "@types/node": "12.11.7",
    "@types/react": "16.9.17",
    "@types/react-dom": "16.9.4",
    "antd": "4.0.0-rc.1",
    "formik": "2.1.2",
    "jest": "24.9.0",
    "moment": "2.24.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "ts-jest": "24.1.0",
    "tslib": "1.10.0",
    "typescript": "3.7.4",
    "prettier": "1.18.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jannikbuschke/formik-antd/"
  }
}
